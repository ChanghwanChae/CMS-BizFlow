	<parameter name="reqNum" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT


 "S"."SG_CT_DSCR" AS Classification_Type,
 "S"."SG_RT_DSCR" AS Request_Type,
 "S"."POS_BGT_APR_OFM" AS Budget_Auth,
 "S"."POS_CNDT_LAST_NM" AS Candidate_Last_Name,
 "S"."POS_CNDT_FIRST_NM" AS Candidate_First_Name,
  "S"."POS_CNDT_MIDDLE_NM" AS Candidate_Middle_Name,
  "S"."POS_DUAL_EMPLMT" As Dual_Employment,
 NVL("C"."CS_TITLE",  "S"."POS_TITLE")  AS Position_Title,
 NVL("C"."CS_PAY_PLAN_DSCR", "S"."POS_PAY_PLAN_DSCR") AS Pay_Plan,
 CASE
 		WHEN "CP"."PROCESS_STATE_DESC" ='Completed'
 		THEN  SUBSTR("C"."CS_SR_DSCR", 1, 4) 
 		ELSE  "S"."POS_SERIES" 
 END AS Series,
 
 NVL("C"."CS_SEC_DSCR",  "S"."POS_SEC_DSCR")  AS Position_Sensitivity,
 NVL("C"."CS_SUPERVISORY_DSCR", "S"."POS_SUPERVISORY_DSCR") AS Position_Is,

 "S"."POS_CAN" AS CAN,
 "S"."POS_VICE" AS Vice,
 "S"."POS_VICE_NAME" AS Vice_Name,

 "S"."POS_LOCATION_DSCR"  AS Duty_Station,
 "S"."POS_REPORT_SUPERVISOR_NAME" AS Supervisor,

 "S"."POS_AT_DSCR" AS Type_Appointment,
 "S"."POS_NTE" AS NTE,
 "S"."POS_WORK_SCHED_DSCR" AS Work_Schedule,
 "S"."POS_HOURS_PER_WEEK" AS Hours_Week,
 
 CASE 
 		WHEN  "CP"."PROCESS_STATE_DESC" ='Completed'
 		THEN "C"."CS_FIN_STMT_REQ_DSCR" 
 		ELSE  "S"."POS_CE_FINANCIAL_TYPE_DSCR" 
 END AS Financial_Doc,
 
  "S"."POS_CE_PE_PHYSICAL" AS Physical_Req,
 "S"."POS_CE_DRUG_TEST" AS Drug_Test_Req,
 "S"."POS_CE_IMMUN" AS Immunization_Req,
 "S"."POS_CE_TRAVEL" AS Travel_Req,
 "S"."POS_CE_TRAVEL_PER" AS Travel_Perc,
 "S"."POS_CE_LIC" AS Licence_Req,
 "S"."POS_CE_LIC_INFO" AS License_Info, 
 

 "S"."POS_REMARKS" AS Remarks
  
 
FROM  "HHS_CMS_HR"."VW_STRATCON" S 
LEFT JOIN  "HHS_CMS_HR"."VW_CLASSIFICATION" C ON  C."REQ_ID" = S."REQ_ID" -- joined with Classification form data
LEFT JOIN  "HHS_VW_CLASSIFICATION_PROC" CP ON  CP."PROCESS_ID" = C."PD_PROCID" -- and Classification process data

WHERE  "S"."REQ_JOB_REQ_NUMBER" = $P{reqNum}]]>
	</queryString>
