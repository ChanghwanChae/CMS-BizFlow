	<queryString>
		<![CDATA[SELECT 
	INC."CANDI_NAME"  AS Applicant_Name,
	INC."ADMIN_CODE" AS Admin_Code,
	INC."ORG_NAME" AS Organization,
	INC."POS_TITLE" AS Position_Title,
 	INC."PAY_PLAN" AS Pay_Plan,
	INC."SERIES" AS Series,
			
	CASE 	WHEN INC."PAY_PLAN" IN ('ES') THEN '00'
				WHEN INC."PAY_PLAN" IN ('GP', 'GR', 'GS', 'WG') THEN INC."GRADE" 
				ELSE 'N/A' END AS Grade,
	
	SAM."JUSTIFICATION_SUPER_QUAL_DESC" As Desc_Superior_Qual,
	SAM."JUSTIFICATION_QUAL_COMP_DESC" As Desc_Qual_Compared, 
	SAM."JUSTIFICATION_PAY_EQUITY_DESC" As Desc_Pay_Equity,
	SAM."JUSTIFICATION_EXIST_PKG_DESC" As Desc_Existing_Comp,
 	SAM."JUSTIFICATION_EXPLAIN_CONSID" As Desc_Explain,
 	
	INC."DS_CITY" As City,
	INC."DS_STATE" As State,
	CASE WHEN INC."PAY_PLAN" IN ('ED', 'EE', 'EF', 'EG', 'ES', 'EX') THEN 'N/A'
				ELSE SAM."RCMD_SALARY_STEP" END AS Rec_Step,
	SAM."RCMD_SALARY_SALARY_PER_ANNUM"  AS Rec_Salary,
	SAM."RCMD_SALARY_LOCALITY_PAY_SCALE" AS Rec_Locality,
	
	SAM."APPROVAL_SO" As Appr_SO_Name,
	SAM."APPROVAL_SO_RESP_DATE" As Appr_SO_Date,
	SAM."APPROVAL_SO_ACTING" As Appr_SO_Acting,
	INC."SO_TITLE" As SO_Title,
	
	SAM."APPROVAL_COC" As Appr_COC_Name,
	SAM."APPROVAL_COC_RESP_DATE" As Appr_COC_Date,
    SAM."APPROVAL_COC_ACTING" As Appr_COC_Acting,
	
	SAM."RVW_HRS" As  HRS_Reviewer_Name,
	SAM."HRS_RVW_DATE" As HRS_Review_Date,
    SAM."HRS_RVW_CERT" As HRS_Review_Resp,
    SAM."HRS_NOT_SPT_RSN" As HRS_Review_Nonsupport,

	SAM."APPROVAL_TABG" As Appr_TABG_Name,
	NVL(SAM."APPROVAL_TABG_VALUE", 'TBD') As Appr_TABG_Resp,
	SAM."APPROVAL_TABG_RESP_DATE" As Appr_TABG_Review_Date,
	SAM."APPROVAL_TABG_ACTING" As Appr_TABG_Acting,
	
	SAM."APPROVER_NOTES" As Approval_Notes

  
FROM HHS_CMS_HR.VW_INCENTIVES_COM INC
INNER JOIN HHS_CMS_HR.VW_INCENTIVES_SAM SAM ON   INC.PROC_ID = SAM.PROC_ID


WHERE INC.PROC_ID = $P{caseID}]]>
	</queryString>
