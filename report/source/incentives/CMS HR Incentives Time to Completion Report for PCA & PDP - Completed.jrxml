	<queryString>
		<![CDATA[--    CMS HR Incentives Time to Completion Report for PCA & PDP - Completed

SELECT
    PCAPDP.PROCID  
  , PCAPDP.STATE
  , PCAPDP.NAME
  , PCAPDP.CREATIONDTIME
  , PCAPDP.DATE_COMPLETED
  , PCAPDP.PARENT_PROC
  , PCAPDP.DATE_CREATED
  , PCAPDP.OHC_REVIEW_DATE
  , PCAPDP.ADMINISTRATOR_APPROVAL_DATE
  , PCAPDP.DATE_OF_RECOMMENDATION
  , PCAPDP.DATE_OF_MEETING
  , PCAPDP.REQUEST_NUM
  , PCAPDP.ADMIN_CODE
  , PCAPDP.REQUEST_TYPE
  , PCAPDP.CANCEL_RESAON
  , PCAPDP.APPOINTMENT_TYPE
  , PCAPDP.SCHEDULE_A_TYPE
  , PCAPDP.REQ_DATE  
  , PCAPDP.INCENTIVE_TYPE
  , PCAPDP.CANDIDATE_NAME
   ,  CASE WHEN PCAPDP.PAY_PLAN IS NOT NULL THEN PCAPDP.PAY_PLAN ELSE 'N/A' END AS PAY_PLAN
   ,  CASE WHEN PCAPDP.SERIES IS NOT NULL THEN PCAPDP.SERIES ELSE 'N/A' END AS SERIES
   ,  CASE WHEN PCAPDP.GRADE IS NOT NULL THEN PCAPDP.GRADE ELSE 'N/A' END AS GRADE  , PCAPDP.HRS1_ID
  
   , CASE WHEN $P{DAYS} = 'Business' THEN HHS_FN_GET_BUSDAYSDIFF( 
        (SELECT DATE_COMPLETED FROM BIZFLOW.PROCS WHERE PROCID = ( SELECT INDEXVALUE FROM BIZFLOW.RLVNTDATA WHERE PROCID = PCAPDP.PROCID AND RLVNTDATANAME='parentProcId')),
        (SELECT EXECDTIME FROM BIZFLOW.AUDITINFO WHERE PROCID = PCAPDP.PROCID AND EVENT = 'I' AND OBJNAME='Selecting Official Reviews Request') )
      ELSE
        ( SELECT EXECDTIME FROM BIZFLOW.AUDITINFO WHERE PROCID = PCAPDP.PROCID AND EVENT = 'I' AND OBJNAME='Selecting Official Reviews Request') - 
        ( SELECT DATE_COMPLETED FROM BIZFLOW.PROCS WHERE PROCID = (SELECT INDEXVALUE FROM BIZFLOW.RLVNTDATA WHERE PROCID = PCAPDP.PROCID AND RLVNTDATANAME='parentProcId') ) 
     END AS Time_HRS_Create
     
   , CASE WHEN $P{DAYS} = 'Business' THEN HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Selecting Official Reviews Request')
      ELSE HHS_FN_GET_ACCCALDAYSDIFF(PCAPDP.PROCID,'Selecting Official Reviews Request')
     END AS Draft_Just_Busday
     
   , CASE WHEN $P{DAYS} = 'Business' THEN HHS_FN_GET_BUSDAYSDIFF(
        (SELECT EXECDTIME FROM BIZFLOW.AUDITINFO WHERE PROCID = PCAPDP.PROCID AND EVENT = 'I' AND OBJNAME='Selecting Official Reviews Request'),
        PCAPDP.OHC_REVIEW_DATE )
      ELSE PCAPDP.OHC_REVIEW_DATE - (SELECT EXECDTIME FROM BIZFLOW.AUDITINFO WHERE PROCID = PCAPDP.PROCID AND EVENT = 'I' AND OBJNAME='HR Specialist Review and Approval Request')
     END AS Finalize_Req_Busday

   , CASE WHEN $P{DAYS} = 'Business' AND PCAPDP.NAME = 'PDP Incentives' THEN HHS_FN_GET_BUSDAYSDIFF(PCAPDP.DATE_OF_MEETING, PCAPDP.DATE_OF_RECOMMENDATION)
          WHEN $P{DAYS} = 'Calendar' AND PCAPDP.NAME = 'PDP Incentives' THEN PCAPDP.DATE_OF_RECOMMENDATION - PCAPDP.DATE_OF_MEETING
      ELSE 0.00
     END AS Conduct_Panel_Rvw_Busday

   , CASE WHEN $P{DAYS} = 'Business' THEN HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Selecting Official Reviews Request for Modification')
      ELSE HHS_FN_GET_ACCCALDAYSDIFF(PCAPDP.PROCID,'Selecting Official Reviews Request for Modification')
     END AS Modify_Init_Just_Busday

   , CASE WHEN $P{DAYS} = 'Business' THEN HHS_FN_GET_BUSDAYSDIFF(
        (SELECT EXECDTIME FROM BIZFLOW.AUDITINFO WHERE PROCID = PCAPDP.PROCID AND EVENT = 'I' AND OBJNAME = 'HR Specialist Review and Approval Request'),
        PCAPDP.ADMINISTRATOR_APPROVAL_DATE )
      ELSE PCAPDP.ADMINISTRATOR_APPROVAL_DATE - ( SELECT EXECDTIME FROM BIZFLOW.AUDITINFO WHERE PROCID = PCAPDP.PROCID AND EVENT = 'I' AND OBJNAME = 'HR Specialist Review and Approval Request')
     END AS Obtain_Fnl_Apprvl_Busday     
   
   , HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Create Request') AS CREATE_REQ_BUSDAY
   , HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Draft Justification') AS DRAFT_JUST_BUSDAY
   , HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Finalize Request') AS FINALIZE_REQ_BUSDAY
   , HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Conduct Panel Review') AS CONDUCT_PANEL_RVW_BUSDAY
   , HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Modify Initial Justification') AS MODIFY_INIT_JUST_BUSDAY
   , HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Obtain Final Approval') AS OBTAIN_FNL_APPRVL_BUSDAY
       
   , CASE WHEN $P{DAYS} = 'Calendar' THEN ROUND(PCAPDP.DATE_COMPLETED - PCAPDP.CREATIONDTIME)
     ELSE 
     BIZFLOW.HHS_FN_GET_BUSDAYSDIFF(PCAPDP.CREATIONDTIME, PCAPDP.DATE_COMPLETED) END AS PROCESS_AGE

   ,(SELECT (EXTRACTVALUE(DTL.field_data, '/formData /items/item[id="staffingSpecialist"]/value/name') || ' ' || EXTRACTVALUE(DTL.field_data, '/formData    /items/item[id="staffingSpecialist"]/value/org') ) 
   FROM HHS_CMS_HR.TBL_FORM_DTL DTL WHERE DTL.PROCID = PCAPDP.PROCID) AS HR_SPECIALIST

   ,(SELECT (EXTRACTVALUE(DTL.field_data, '/formData /items/item[id="staffingSpecialist"]/value/id')  ) 
   FROM HHS_CMS_HR.TBL_FORM_DTL DTL WHERE DTL.PROCID = PCAPDP.PROCID) AS HRS_ID
   
   , HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Component Reviews Request') 
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Component Reviews Request for Modification') 
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Selecting Official Reviews Request') 
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'Center/Office/Consortium Reviews Request') AS TOP_Component


   , CASE WHEN $P{DAYS} = 'Business' THEN        
     HHS_FN_GET_ACCBUSDAYSDIFF(FN_GET_RLVNTDATAVALUE(PCAPDP.PROCID, 'N', 'S', 'parentProcId'),'Start')
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'HR Specialist Reviews Request for Modification') 
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'HR Specialist Reviews Request')
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'TABG Division Director Reviews Request') 
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'TABG Director/Deputy Director Reviews Request') 
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'OHC Director/Deputy Director Reviews Request')
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'HR Specialist Final Review of Documents')
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'HR Specialist Final Review of Documents')
       + HHS_FN_GET_BUSDAYSDIFF(PCAPDP.DATE_OF_MEETING, PCAPDP.DATE_OF_RECOMMENDATION)
     ELSE HHS_FN_GET_ACCBUSDAYSDIFF(FN_GET_RLVNTDATAVALUE(PCAPDP.PROCID, 'N', 'S', 'parentProcId'),'Start')
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'HR Specialist Reviews Request for Modification') 
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'HR Specialist Reviews Request')
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'TABG Division Director Reviews Request') 
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'TABG Director/Deputy Director Reviews Request') 
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'OHC Director/Deputy Director Reviews Request')
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'HR Specialist Final Review of Documents')
       + HHS_FN_GET_ACCBUSDAYSDIFF(PCAPDP.PROCID,'HR Specialist Final Review of Documents')
       + PCAPDP.DATE_OF_RECOMMENDATION - PCAPDP.DATE_OF_MEETING END AS TOP_HR

FROM
(
SELECT
     P.PROCID AS PROCID
   , P.STATE AS STATE
   , P.NAME AS NAME     
   , P.CREATIONDTIME AS CREATIONDTIME
   , P.CMPLTDTIME AS DATE_COMPLETED
   , FN_GET_RLVNTDATAVALUE(P.PROCID, 'I', 'N', 'parentProcId') AS PARENT_PROC
   , (SELECT CREATIONDTIME FROM BIZFLOW.PROCS WHERE PROCID =  FN_GET_RLVNTDATAVALUE(P.PROCID, 'I', 'N', 'parentProcId') ) AS DATE_CREATED   

   , PCA.APPROVAL_OADMIN_DATE_D AS OHC_REVIEW_DATE
   , PCA.APPROVAL_OADMIN_DATE_D AS ADMINISTRATOR_APPROVAL_DATE
   , SYSDATE AS DATE_OF_RECOMMENDATION
   , SYSDATE AS DATE_OF_MEETING

   , COM.REQ_NUM AS REQUEST_NUM
   , COM.ADMIN_CODE  AS ADMIN_CODE
   , COM.REQ_TYPE   AS REQUEST_TYPE
   , COM.CANCEL_RESAON AS CANCEL_RESAON
   , SCF.SG_AT_DSCR   AS APPOINTMENT_TYPE
   , SCF.SG_SAT_DSCR   AS SCHEDULE_A_TYPE
   , COM.REQ_DATE AS REQ_DATE
   , COM.INCEN_TYPE  AS INCENTIVE_TYPE
   , COM.CANDI_NAME  AS CANDIDATE_NAME	 
   , COM.PAY_PLAN  AS PAY_PLAN
   , COM.SERIES  AS SERIES
   , CASE WHEN COM.PAY_PLAN  IN ('ES', 'GP', 'GR', 'GS', 'WG') THEN COM.GRADE  ELSE 'N/A' END AS GRADE
   ,(SELECT (EXTRACTVALUE(DTL.field_data, '/formData /items/item[id="staffingSpecialist"]/value/name') || ' ' || EXTRACTVALUE(DTL.field_data, '/formData /items/item[id="staffingSpecialist"]/value/org') ) FROM HHS_CMS_HR.TBL_FORM_DTL DTL WHERE DTL.PROCID = P.PROCID) AS HR_SPECIALIST   
   ,(SELECT (EXTRACTVALUE(DTL.field_data, '/formData /items/item[id="staffingSpecialist"]/value/id')  ) 
   FROM HHS_CMS_HR.TBL_FORM_DTL DTL WHERE DTL.PROCID = P.PROCID) AS HRS1_ID
 FROM
  BIZFLOW.PROCS P
  INNER JOIN   HHS_CMS_HR .VW_INCENTIVES_COM  COM ON P.PROCID = COM.PROC_ID
  INNER JOIN  HHS_CMS_HR .VW_STRATCON  SCF ON  COM.REQ_NUM = SCF.REQ_JOB_REQ_NUMBER
  INNER JOIN HHS_CMS_HR.VW_INCENTIVES_PCA PCA ON COM.PROC_ID = PCA.PROC_ID
  WHERE  P.NAME IN ('PCA Incentives')
  AND P.STATE = 'C' AND  COM .CANCEL_RESAON  IS NULL    -- Request is completed but not cancelled  

  UNION

SELECT
     P.PROCID AS PROCID
   , P.STATE AS STATE
   , P.NAME AS NAME     
   , P.CREATIONDTIME AS CREATIONDTIME
   , P.CMPLTDTIME AS DATE_COMPLETED
   , FN_GET_RLVNTDATAVALUE(P.PROCID, 'I', 'N', 'parentProcId') AS PARENT_PROC
   , (SELECT CREATIONDTIME FROM BIZFLOW.PROCS WHERE PROCID =  FN_GET_RLVNTDATAVALUE(P.PROCID, 'I', 'N', 'parentProcId') ) AS DATE_CREATED   

   , PDP.OHC_REVIEW_DATE AS OHC_REVIEW_DATE
   , PDP.ADMINISTRATOR_APPROVAL_DATE AS ADMINISTRATOR_APPROVAL_DATE
   , PDP.DATE_OF_RECOMMENDATION AS DATE_OF_RECOMMENDATION
   , PDP.DATE_OF_MEETING AS DATE_OF_MEETING

   , COM.REQ_NUM AS REQUEST_NUM
   , COM.ADMIN_CODE  AS ADMIN_CODE
   , COM.REQ_TYPE   AS REQUEST_TYPE
   , COM.CANCEL_RESAON AS CANCEL_RESAON
   , SCF.SG_AT_DSCR   AS APPOINTMENT_TYPE
   , SCF.SG_SAT_DSCR   AS SCHEDULE_A_TYPE
   , COM.REQ_DATE AS REQ_DATE
   , COM.INCEN_TYPE  AS INCENTIVE_TYPE
   , COM.CANDI_NAME  AS CANDIDATE_NAME	 
   , COM.PAY_PLAN  AS PAY_PLAN
   , COM.SERIES  AS SERIES
   , CASE WHEN COM.PAY_PLAN  IN ('ES', 'GP', 'GR', 'GS', 'WG') THEN COM.GRADE  ELSE 'N/A' END AS GRADE
   ,(SELECT (EXTRACTVALUE(DTL.field_data, '/formData /items/item[id="staffingSpecialist"]/value/name') || ' ' || EXTRACTVALUE(DTL.field_data, '/formData /items/item[id="staffingSpecialist"]/value/org') ) FROM HHS_CMS_HR.TBL_FORM_DTL DTL WHERE DTL.PROCID = P.PROCID) AS HR_SPECIALIST   
   ,(SELECT (EXTRACTVALUE(DTL.field_data, '/formData /items/item[id="staffingSpecialist"]/value/id')  ) 
   FROM HHS_CMS_HR.TBL_FORM_DTL DTL WHERE DTL.PROCID = P.PROCID) AS HRS1_ID   
 FROM
  BIZFLOW.PROCS P
  INNER JOIN   HHS_CMS_HR .VW_INCENTIVES_COM  COM ON P.PROCID = COM.PROC_ID
  INNER JOIN  HHS_CMS_HR .VW_STRATCON  SCF ON  COM.REQ_NUM = SCF.REQ_JOB_REQ_NUMBER
  INNER JOIN HHS_CMS_HR.VW_INCENTIVES_PDP PDP ON COM.PROC_ID = PDP.PROC_ID  
  WHERE  P.NAME IN ('PDP Incentives')
  AND P.STATE = 'C' AND  COM .CANCEL_RESAON  IS NULL    -- Request is completed but not cancelled
  ) PCAPDP

 WHERE PCAPDP.STATE = 'C' AND PCAPDP."CANCEL_RESAON" IS NULL    -- Request is completed but not cancelled
 AND PCAPDP.NAME IN ('PCA Incentives', 'PDP Incentives')       -- PCA and PDP Incentives only
  
  -- MANDATORY filters
                 
 	AND (
            	(($P{REPORT_MODE} = 'By Component' 
                	AND	( 	($P{COMPONENT} = 'CMS-wide' AND PCAPDP.ADMIN_CODE LIKE 'FC%') 
                          	  	OR ($P{COMPONENT} = 'Office of the Administrator (OA) Only' AND PCAPDP.ADMIN_CODE = 'FC')
                            	OR ($P{COMPONENT} = 'By Admin Code' AND $P{INC_SUBORG} = 'Yes' AND PCAPDP.ADMIN_CODE LIKE '$P!{ADMIN_CD}' || '%')
                            	OR ($P{COMPONENT} = 'By Admin Code' AND $P{INC_SUBORG} = 'No' AND PCAPDP.ADMIN_CODE = $P{ADMIN_CD})
                    			)
            	 )  -- Component filter: CMS-wide; FC only; Admin Code with children; Admin Code w/o children
            
                	AND TO_DATE(PCAPDP.REQ_DATE, 'YYYY/MM/DD') BETWEEN $P{DATE_FROM} AND $P{DATE_TO}) --  Plus Date range filter 
               
            	OR (($P{REPORT_MODE} = 'By Request Num' AND PCAPDP.REQUEST_NUM = $P{REQ_NUM}) -- Request Number filter
                	AND TO_DATE(PCAPDP.REQ_DATE, 'YYYY/MM/DD') BETWEEN NVL($P{DATE_FROM}, '01-JAN-2017') AND NVL($P{DATE_TO}, SYSDATE)) --  Plus Date range filter
      )         
               

  -- OPTIONAL filters
               
  AND (($P{REQ_TYPE} = 'All') OR (PCAPDP.REQUEST_TYPE = $P{REQ_TYPE}))       -- Request Type filter: All Request Types or a specific type
  AND (($P{APPT_TYPE} = 'All') OR (PCAPDP.APPOINTMENT_TYPE = $P{APPT_TYPE}))     -- Appointment Type filter: All Appointment Types or a specific type

  AND $X{IN, PCAPDP.INCENTIVE_TYPE, INCEN_TYPE} -- Incentive Type filter 

  AND (($P{HRS_ID} = 'All') OR (PCAPDP.HRS1_ID = $P{HRS_ID}))              -- HR Specialist
 	
	ORDER BY DATE_CREATED]]>
	</queryString>
	<field name="PROCID" class="java.math.BigDecimal"/>
	<field name="STATE" class="java.lang.String"/>
	<field name="NAME" class="java.lang.String"/>
	<field name="CREATIONDTIME" class="java.sql.Timestamp"/>
