	<field name="REQ_DATE" class="java.lang.String"/>
	<field name="INCENTIVE_TYPE" class="java.lang.String"/>
	<field name="CANDIDATE_NAME" class="java.lang.String"/>
	<field name="PAY_PLAN" class="java.lang.String"/>
	<field name="SERIES" class="java.lang.String"/>
	<field name="GRADE" class="java.lang.String"/>
	<field name="HRS1_ID" class="java.lang.String"/>
	<field name="TIME_HRS_CREATE" class="java.math.BigDecimal"/>
	<field name="DRAFT_JUST_BUSDAY" class="java.math.BigDecimal"/>
	<field name="FINALIZE_REQ_BUSDAY" class="java.math.BigDecimal"/>
	<field name="CONDUCT_PANEL_RVW_BUSDAY" class="java.math.BigDecimal"/>
	<field name="MODIFY_INIT_JUST_BUSDAY" class="java.math.BigDecimal"/>
	<field name="OBTAIN_FNL_APPRVL_BUSDAY" class="java.math.BigDecimal"/>
	<field name="CREATE_REQ_BUSDAY" class="java.math.BigDecimal"/>
	<field name="COLUMN_32" class="java.math.BigDecimal"/>
	<field name="COLUMN_33" class="java.math.BigDecimal"/>
	<field name="COLUMN_34" class="java.math.BigDecimal"/>
	<field name="COLUMN_35" class="java.math.BigDecimal"/>
	<field name="COLUMN_36" class="java.math.BigDecimal"/>
	<field name="PROCESS_AGE" class="java.math.BigDecimal"/>
	<field name="HR_SPECIALIST" class="java.lang.String"/>
	<field name="HRS_ID" class="java.lang.String"/>
	<field name="TOP_COMPONENT" class="java.math.BigDecimal"/>
	<field name="TOP_HR" class="java.math.BigDecimal"/>
	<variable name="Avg_Days_Complete" class="java.lang.Integer" calculation="Average">
		<variableExpression><![CDATA[new Boolean( $F{PROCESS_AGE} != null ) ? $F{PROCESS_AGE} : 0.00]]></variableExpression>
	</variable>
	<variable name="Avg_Days_TOP_HR" class="java.lang.Integer" calculation="Average">
		<variableExpression><![CDATA[$F{TOP_HR}]]></variableExpression>
	</variable>
	<variable name="Avg_Days_TOP_Comp" class="java.lang.Integer" calculation="Average">
		<variableExpression><![CDATA[$F{TOP_COMPONENT}]]></variableExpression>
	</variable>
