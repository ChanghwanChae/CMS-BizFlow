	<queryString>
		<![CDATA[SELECT
	USAS_Req_Num,
	Create_Date,
	Request_Status,
	Position_Title,
	Pay_Plan,
	Series,
	Grade,
	Send_Tent_Offer_Date,
	Init_Background_Inv_Date,
	Rec_Background_Inv_Date,
	Send_Offer_Complete_Date,
	New_Hire_Name,
	Eod_Date,
	Area_Consideration,
	Days_To_Offer,
	BF_Req_Num,
	PD_Num_1,
	PD_Num_2,
	PD_Num_3,
	PD_Num_4,
	PD_Num_5,
	Admin_Code,
	Second_Sub_Org,
	BF_Request_Type,
	BF_Appoint_Type,
	Schedule_A_Type,
	Volunteer_Type,
	CASE
		WHEN $P{DAYS} = 'Calendar Days'
		THEN
			CASE
				WHEN SAM_STARTDATE >= LE_STARTDATE AND LE_ENDDATE<=SAM_ENDDATE			
					THEN ROUND(SAM_ENDDATE - SAM_STARTDATE)
				WHEN SAM_STARTDATE>= LE_STARTDATE AND SAM_ENDDATE<= LE_ENDDATE			
					THEN ROUND(SAM_ENDDATE - LE_STARTDATE)
				WHEN LE_STARTDATE >= SAM_STARTDATE AND LE_ENDDATE <= SAM_ENDDATE				
					THEN ROUND(LE_ENDDATE - SAM_STARTDATE)
				WHEN LE_STARTDATE >= SAM_STARTDATE AND SAM_ENDDATE <= LE_ENDDATE		
					THEN ROUND(LE_ENDDATE - LE_STARTDATE)			
				WHEN SAM_STARTDATE IS NOT NULL AND SAM_ENDDATE IS NOT NULL
					AND LE_STARTDATE IS NOT NULL AND LE_ENDDATE IS NOT NULL
					THEN ROUND(LE_ENDDATE - LE_STARTDATE) + ROUND(SAM_ENDDATE - SAM_STARTDATE)
				WHEN SAM_STARTDATE IS NOT NULL AND SAM_ENDDATE IS NOT NULL
					AND LE_STARTDATE IS NULL AND LE_ENDDATE IS NULL
					THEN ROUND(SAM_ENDDATE - SAM_STARTDATE)
				WHEN SAM_STARTDATE IS NULL AND SAM_ENDDATE IS NULL
					AND LE_STARTDATE IS NOT NULL AND LE_ENDDATE IS NOT NULL
					THEN ROUND(LE_ENDDATE - LE_STARTDATE)				
			END	
		ELSE
			CASE
				WHEN SAM_STARTDATE >= LE_STARTDATE AND LE_ENDDATE<=SAM_ENDDATE
					THEN BIZFLOW.HHS_FN_GET_BUSDAYSDIFF(SAM_STARTDATE, SAM_ENDDATE)
				WHEN SAM_STARTDATE>= LE_STARTDATE AND SAM_ENDDATE<= LE_ENDDATE			
					THEN BIZFLOW.HHS_FN_GET_BUSDAYSDIFF(SAM_STARTDATE, LE_ENDDATE)
				WHEN LE_STARTDATE >= SAM_STARTDATE AND LE_ENDDATE <= SAM_ENDDATE			
					THEN BIZFLOW.HHS_FN_GET_BUSDAYSDIFF(LE_STARTDATE, SAM_ENDDATE)
				WHEN LE_STARTDATE >= SAM_STARTDATE AND SAM_ENDDATE <= LE_ENDDATE			
					THEN BIZFLOW.HHS_FN_GET_BUSDAYSDIFF(LE_STARTDATE, LE_ENDDATE)
				WHEN SAM_STARTDATE IS NOT NULL AND SAM_ENDDATE IS NOT NULL
					AND LE_STARTDATE IS NOT NULL AND LE_ENDDATE IS NOT NULL
					THEN BIZFLOW.HHS_FN_GET_BUSDAYSDIFF(LE_STARTDATE, LE_ENDDATE) + BIZFLOW.HHS_FN_GET_BUSDAYSDIFF(SAM_STARTDATE, SAM_ENDDATE)
				WHEN SAM_STARTDATE IS NOT NULL AND SAM_ENDDATE IS NOT NULL
					AND LE_STARTDATE IS NULL AND LE_ENDDATE IS NULL
					THEN BIZFLOW.HHS_FN_GET_BUSDAYSDIFF(SAM_STARTDATE, SAM_ENDDATE)
				WHEN SAM_STARTDATE IS NULL AND SAM_ENDDATE IS NULL
					AND LE_STARTDATE IS NOT NULL AND LE_ENDDATE IS NOT NULL
					THEN BIZFLOW.HHS_FN_GET_BUSDAYSDIFF(LE_STARTDATE, LE_ENDDATE)					
			END		
	END AS SAM_LE_DAYS
FROM	
	(
		SELECT 
			USAS."REQUEST_NUMBER" AS USAS_Req_Num,
			USAS."NEW_HIRE_CREATE_DATE" AS Create_Date,
			USAS."REQUEST_STATUS" AS Request_Status,
			USAS."POSITION_TITLE" AS Position_Title,
			USAS."NEW_HIRE_PAY_PLAN" AS Pay_Plan,
			USAS."NEW_HIRE_SERIES" AS Series,
			LPAD( USAS."NEW_HIRE_GRADE", 2, '0')  AS Grade,
			USAS."SEND_TENT_OFFR_CMPL_DATE" AS Send_Tent_Offer_Date,
			USAS."INIT_BKGRND_INVST_DATE" AS Init_Background_Inv_Date,
			USAS."RCVE_BKGRND_INVST_DATE" AS Rec_Background_Inv_Date,
			USAS."SEND_OFCL_OFFR_CMPL_DATE" AS Send_Offer_Complete_Date,
			USAS."NEW_HIRE_NAME" AS New_Hire_Name,
			USAS."EOD_DATE" AS Eod_Date,
			
		(SELECT  "HHS_HR"."AREA_CONSIDERATION"."AREA_CONSIDER_DESC" 
				FROM  "HHS_HR"."AREA_CONSIDERATION" 
				WHERE  "HHS_HR"."AREA_CONSIDERATION"."AREA_CONSIDER_CODE" = USAS."AREA_CONSIDER"
		) AS Area_Consideration,
			
		CASE
			WHEN $P{DAYS} = 'Calendar Days' 
			THEN ROUND(USAS."SEND_OFCL_OFFR_CMPL_DATE" - USAS."NEW_HIRE_CREATE_DATE" ) 
			ELSE BIZFLOW.HHS_FN_GET_BUSDAYSDIFF(USAS."NEW_HIRE_CREATE_DATE", USAS."SEND_OFCL_OFFR_CMPL_DATE") 
		END AS Days_To_Offer,
		
			SCF."REQ_JOB_REQ_NUMBER" AS BF_Req_Num,
			
		CLF."CS_PD_NUMBER_JOBCD_1" AS PD_Num_1,
		CLF."CS_PD_NUMBER_JOBCD_2" AS PD_Num_2,
		CLF."CS_PD_NUMBER_JOBCD_3" AS PD_Num_3,
		CLF."CS_PD_NUMBER_JOBCD_4" AS PD_Num_4,
		CLF."CS_PD_NUMBER_JOBCD_5" AS PD_Num_5,
				
		CASE
				WHEN SCF."SG_RT_DSCR"  = 'Recruitment'
				THEN CLF."CS_ADMIN_CD"
				ELSE EQF."ADMIN_CD"
				END AS Admin_Code,
				
			CASE
				WHEN SCF."SG_RT_DSCR"  = 'Recruitment'
				THEN  CLF."PD_SUB_ORG_DSCR_2" 
				ELSE EQF."SUB_ORG_2_DSCR"
				END AS Second_Sub_Org,
				
			SCF."SG_RT_DSCR" AS BF_Request_Type,
			SCF."SG_AT_DSCR"  AS BF_Appoint_Type,
			SCF."SG_SAT_DSCR" AS Schedule_A_Type,
			SCF."SG_VT_DSCR" AS Volunteer_Type,
			
			(SELECT EXECDTIME FROM BIZFLOW.AUDITINFO WHERE PROCID = BIZFLOW.FN_GET_RLVNTDATAVALUE(SAM.PROC_ID, 'I', 'N', 'parentProcId') 
				AND OBJNAME = 'Start New' AND EVENT = 'R' AND OBJTYPE = 'A') AS SAM_STARTDATE,
			(SELECT EXECDTIME FROM BIZFLOW.AUDITINFO WHERE PROCID = BIZFLOW.FN_GET_RLVNTDATAVALUE(LE.PROC_ID, 'I', 'N', 'parentProcId')
				AND OBJNAME = 'Start New' AND EVENT = 'R' AND OBJTYPE = 'A') AS LE_STARTDATE,
			(SELECT CMPLTDTIME FROM BIZFLOW.PROCS WHERE PROCID = SAM.PROC_ID) AS SAM_ENDDATE,
			(SELECT CMPLTDTIME FROM BIZFLOW.PROCS WHERE PROCID = LE.PROC_ID) AS LE_ENDDATE	
		
		FROM  "HHS_HR"."DSS_TIME_TO_OFFER" USAS
			INNER JOIN  "HHS_CMS_HR"."VW_STRATCON" SCF ON SUBSTR(USAS."REQUEST_NUMBER",0,13) = SCF."REQ_JOB_REQ_NUMBER"  --joined to Strat Con form data
			INNER JOIN  "HHS_CMS_HR"."VW_CLASSIFICATION" CLF ON CLF.REQ_ID = SCF.REQ_ID  --joined to Classification form data
			INNER JOIN  "HHS_VW_CLASSIFICATION_PROC" CLP ON  CLP."PROCESS_ID" =  CLF."PD_PROCID" -- joined to Classification process data
			INNER JOIN  "HHS_CMS_HR"."VW_ELIGQUAL" EQF ON  EQF.REQ_ID = SCF.REQ_ID  -- joined to Elig/Qual Review form data
			LEFT OUTER JOIN  "HHS_VW_ELIGQUAL_PROC" EQP ON  EQP."PROCESS_ID" = EQF."PROCID" -- joined to Elig/Qual Review process data
			LEFT JOIN "HHS_CMS_HR"."VW_INCENTIVES_COM" SAM ON SUBSTR(USAS."REQUEST_NUMBER",0,13) = SAM."REQ_NUM" AND UPPER(USAS.NEW_HIRE_NAME) = UPPER(SAM.CANDI_NAME2) AND SAM.INCEN_TYPE = 'SAM' --joined to SAM Incentives data
			LEFT JOIN "HHS_CMS_HR"."VW_INCENTIVES_COM" LE ON SUBSTR(USAS."REQUEST_NUMBER",0,13) = LE."REQ_NUM" AND UPPER(USAS.NEW_HIRE_NAME) = UPPER(LE.CANDI_NAME2) AND LE.INCEN_TYPE = 'LE' --joined to LE Incentives data       
		
		WHERE ((SCF."SG_RT_DSCR"  = 'Appointment' AND "EQP"."PROCESS_STATE_DESC" = 'Completed' AND  NVL("EQP"."REQUEST_STATUS", 'Nothing') <> 'Request Cancelled'
						AND EQP."REQUEST_STATUS" IN ('Appointment Approved', 'Selected by Component', 'Eligibility and Qualifications Completed')) -- Appointment process Completed, but not Cancelled and Candidate Selected
			OR 
						(SCF."SG_RT_DSCR"  = 'Recruitment' AND "CLP"."PROCESS_STATE_DESC" = 'Completed' AND  NVL("CLP"."REQUEST_STATUS", 'Nothing') <> 'Request Cancelled') -- Appointment process Completed, but not Cancelled and Candidate Selected
					)
			AND  
			USAS."REQUEST_STATUS" = 'Complete'			
			
			AND NOT EXISTS (SELECT 1 FROM BIZFLOW.PROCS WHERE PROCID = LE.PROC_ID AND NAME = 'Incentives Request') --Removes duplicate LE data
			AND NOT EXISTS (SELECT 1 FROM BIZFLOW.PROCS WHERE PROCID = SAM.PROC_ID AND NAME = 'Incentives Request') --Removes duplicate SAM data
			
			AND USAS."SEND_OFCL_OFFR_CMPL_DATE" BETWEEN SYS_EXTRACT_UTC(TO_TIMESTAMP(TO_CHAR($P{COMP_DATE_FROM}, 'YYYY-MM-DD') || ' 00:00:00', 'YYYY-MM-DD HH24:MI:SS')) 
			AND SYS_EXTRACT_UTC(TO_TIMESTAMP(TO_CHAR($P{COMP_DATE_TO}, 'YYYY-MM-DD') || ' 23:59:59', 'YYYY-MM-DD HH24:MI:SS')) -- Send Official Offer Completed in given date range
			
			AND (			($P{COMPONENT} = 'CMS-wide' AND ((SCF."SG_RT_DSCR" = 'Recruitment' AND CLF.CS_ADMIN_CD LIKE 'FC%') -- If Recruitment, match Admin Code from Classification
																							OR (SCF."SG_RT_DSCR" = 'Appointment' AND EQF.ADMIN_CD LIKE 'FC%')) -- If Appointment, match Admin Code from Elig/Qual Review
								)     
						OR ($P{COMPONENT} = 'Office of the Administrator (OA) Only' AND ((SCF."SG_RT_DSCR" = 'Recruitment' AND CLF.CS_ADMIN_CD = 'FC') -- If Recruitment, match Admin Code from Classification
																																						OR (SCF."SG_RT_DSCR" = 'Appointment' AND EQF.ADMIN_CD = 'FC')) -- If Appointment, match Admin Code from Elig/Qual Review
								)
						OR ($P{COMPONENT} = 'By Admin Code' AND $P{INC_SUBORG} = 'Yes' AND ((SCF."SG_RT_DSCR" = 'Recruitment' AND CLF.CS_ADMIN_CD LIKE '$P!{ADMIN_CD}' || '%')-- If Recruitment, match Admin Code from Classification
																																										OR(SCF."SG_RT_DSCR" = 'Appointment' AND EQF.ADMIN_CD LIKE '$P!{ADMIN_CD}' || '%'))	-- If Appointment, match Admin Code from Elig/Qual Review                        																																	  			
								)
						OR ($P{COMPONENT} = 'By Admin Code' AND $P{INC_SUBORG} = 'No' AND ((SCF."SG_RT_DSCR" = 'Recruitment' AND CLF.CS_ADMIN_CD = $P{ADMIN_CD})-- If Recruitment, match Admin Code from Classification
																																									OR(SCF."SG_RT_DSCR" = 'Appointment' AND EQF.ADMIN_CD = $P{ADMIN_CD}))	-- If Appointment, match Admin Code from Elig/Qual Review                        																																	  			
								)
					)  -- Admin Code filter: CMS-wide; FC only; Admin Code with children; Admin Code w/o children
			
			
			AND (($P{REQ_TYPE} = 'All') OR (SCF.SG_RT_DSCR  = $P{REQ_TYPE}))      -- Request Type filter: All Request Types or a specific type (Recruitment or Appointment)
			AND (($P{CLSF_TYPE} = 'All') OR ( SCF.SG_CT_DSCR  = $P{CLSF_TYPE}))    -- Classification Type filter: All Classification Types or a specific type
			
			AND (($P{APPT_TYPE} = 'All') OR (EQF.AT_DSCR = $P{APPT_TYPE}))    -- Appointment Type filter: All Appointment Types or a specific type
			AND (($P{SCHDA_TYPE} = 'All') OR (EQF.SAT_DSCR = $P{SCHDA_TYPE})) -- Schedule A filter: All Schedule A Types or a specific type
			AND (($P{VOL_TYPE} = 'All') OR (EQF.VT_DSCR = $P{VOL_TYPE}))      -- Volunteer Type filter: All Volunteer Types or a specific type
		
			AND (($P{SO_ID} = 'All') OR (SCF.SG_RT_DSCR = 'Recruitment' AND SCF.SG_SO_ID = $P{SO_ID}) OR (SCF.SG_CT_DSCR = 'Appointment' AND EQF.SO_ID = $P{SO_ID}))  -- Selecting Official
			AND (($P{XO_ID} = 'All') OR (SCF.SG_CT_DSCR = 'Recruitment' AND SCF.SG_XO_ID = $P{XO_ID}) OR (SCF.SG_CT_DSCR = 'Appointment' AND EQF.XO_ID = $P{XO_ID}))  -- Executive Officer
			AND (($P{HRL_ID} = 'All') OR (SCF.SG_CT_DSCR = 'Recruitment' AND SCF.SG_HRL_ID = $P{HRL_ID}) OR (SCF.SG_CT_DSCR = 'Appointment' AND EQF.HRL_ID = $P{HRL_ID})) -- HR Liaison
			AND (($P{SS_ID} = 'All') OR (SCF.SG_CT_DSCR = 'Recruitment' AND SCF.SG_SS_ID = $P{SS_ID}) OR (SCF.SG_CT_DSCR = 'Appointment' AND EQF.SS_ID = $P{SS_ID}))      -- Staffing Specialist
			AND (($P{CS_ID} = 'All') OR (SCF.SG_CT_DSCR = 'Recruitment' AND SCF.SG_CS_ID = $P{CS_ID}) OR (SCF.SG_CT_DSCR = 'Appointment' AND EQF.CS_ID = $P{CS_ID})) -- Classification Specialist   
		
	)			
ORDER BY Second_Sub_Org, BF_Req_Num]]>
	</queryString>
