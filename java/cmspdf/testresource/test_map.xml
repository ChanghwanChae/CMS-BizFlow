<?xml version="1.0"?>
<map>
	<item name="Employee ID">
		<key>#EMP_ID</key>
		<value>/docRoot/Employee/EmpId</value>
	</item>
	<item name="First Name">
		<key>#FIRST_NM</key>
		<value>/docRoot/Employee/FirstName</value>
	</item>
	<item name="Last Name">
		<key>#LAST_NM</key>
		<value>/docRoot/Employee/LastName</value>
	</item>
	<item name="Gender">
		<key>#GENDER</key>
		<value displayValues="Male|Female" whenValueIs="M">/docRoot/Employee/Gender</value>
	</item>
	<item name="Is Active?">
		<key>#ACTIVE_FL</key>
		<value displayValues="Yes|No" whenValueIs="Y">/docRoot/Employee/IsActive</value>
	</item>
	<item name="Department">
		<key>#DEPT</key>
		<value>/docRoot/Employee/Department</value>
	</item>
	<item name="Pay Plan">
		<key>#PAY_PLAN</key>
		<value lookup="true" lookupField="name">/docRoot/Employee/PayPlan</value>
	</item>
</map>