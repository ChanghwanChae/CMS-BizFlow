{
  "templates": {
    "systemName"            : "BizFlow Project Common Library",
    "includeDate"           : "false",
    "cleverLinks"           : false,
    "monospaceLinks"        : false,
    "default"               : {
      "outputSourceFiles"   : true,
      "includeDate"         : false,
      "copyright"           : "Copyright 2016 BizFlow Corp."
    }
  },
  "tags": {
    "allowUnknownTags" : true
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "recurse" : true,
    "destination": "./out/"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ]

}
