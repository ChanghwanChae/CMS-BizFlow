<?xml version="1.0" encoding="UTF-8"?>
<hy:rulebase cache="false" context="/" disable="true" forward_chain="false" return="/" xmlns="http://www.hyfinity.com/xstore" xmlns:hy="http://www.hyfinity.com/xengine" xmlns:mvc="http://www.hyfinity.com/mvc">
	<hy:rule base="pattern" desc="Writes the page xsl information to be used for screen display" disable="false" id="write_targetPage_case_completed_rule" priority="30" xmlns="http://www.hyfinity.com/xengine">
